                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH ChargeInstanceContext
     AS (
		SELECT  trans.ID      TxnID,
                tlbi.typeCode AS Context,
                chrgp.ChargeCode,
                chrg.chargeGroup,
               -- TransactionEffectiveDate_Ext AS TransactionEffectiveDate,
				SourceSystemProcessDate_Ext AS SourceSystemProcessDate,
				--Reconciled_Ext AS ReconciledDate,
				getdate() AS ReconciledDate,
				TransactionSequence_Ext AS TransactionSequence,
				worsn.NAME as WriteoffReason,
				worsnr.NAME as ReversalWriteoffReason
         FROM   bc_chargeinstancecontext cix
                INNER JOIN bc_transaction trans
                        ON trans.ID = cix.TransactionID
                INNER JOIN bctl_transaction transtyp
                        ON transtyp.ID = trans.Subtype
                INNER JOIN bc_charge chrg
                        ON chrg.ID = cix.ChargeID
                INNER JOIN bc_policyperiod polper
                        ON polper.HiddenTAccountContainerID = chrg.TAccountContainerID
                INNER JOIN bc_chargepattern chrgp
                        ON chrgP.ID = chrg.ChargePatternID
                INNER JOIN bc_TAcctOwnerPattern c
                        ON chrgp.TAccountOwnerPatternID = c.ID
                INNER JOIN bc_billinginstruction bi
                        ON bi.ID = chrg.BillingInstructionID
                INNER JOIN bctl_billinginstruction tlbi
                        ON tlbi.ID = bi.subtype
				LEFT JOIN bc_writeoff wrto
						ON cix.WriteoffID = wrto.ID
				LEFT JOIN bctl_writeoffreason worsn
						ON wrto.Reason = worsn.ID
				LEFT JOIN bc_writeoffreversal wrtor
						ON wrtor.WriteoffID = wrto.ID
				LEFT JOIN bctl_writeoffreversalreason worsnr
						ON wrtor.Reason = worsnr.ID
),
     ProducerContext
     AS (
		SELECT trans.ID      TxnID, 
			trans.TransactionNumber,
			pm.NAME       AS ProducerPaymentMethod,
			tlpp.NAME     AS ProducerPaymentType,
			prodc.NAME    AS ProducerContexttype,
			tlbi.typeCode AS ChargeContext,
			chrgp.ChargeCode,
			chg.ChargeGroup,
		--	TransactionEffectiveDate_Ext AS TransactionEffectiveDate,
			SourceSystemProcessDate_Ext AS SourceSystemProcessDate,
			--Reconciled_Ext AS ReconciledDate,
			getdate() as ReconciledDate,
			TransactionSequence_Ext AS TransactionSequence,
			worsn.NAME as WriteoffReason,
			worsnr.NAME as ReversalWriteoffReason,
			csbp.Name as CommissionSubPlanName,
			prodst.StatementDate as ProducerStatementDate,
			cast(prodst.StatementNumber as varchar(60)) as ProducerStatementNumber
		FROM   bc_producercontext cix
			LEFT JOIN bc_producerstatement prodst
					ON prodst.ID = cix.StatementID
			LEFT JOIN bc_ProducerPayment prodpy
					ON prodpy.ID = cix.ProducerPaymentID
			LEFT JOIN bc_outgoingpayment outp
					ON prodpy.ID = outp.ProducerPaymentID
			LEFT JOIN bc_paymentinstrument pins
					ON outp.PaymentInstrumentID = pins.ID
			LEFT JOIN bctl_paymentmethod pm
					ON pins.PaymentMethod = pm.ID
			LEFT JOIN bc_transaction trans
					ON trans.ID = cix.TransactionID
			INNER JOIN bctl_transaction transtyp
					ON transtyp.ID = trans.Subtype
			LEFT JOIN bc_chargecommission cc
					ON cc.ID = cix.ChargeCommissionID
			LEFT JOIN bc_policycommission polcomm 
					ON polcomm.ID = cc.PolicyCommissionID
			LEFT JOIN bc_commissionsubplan csbp
					ON polcomm.CommissionSubPlanID = csbp.ID
			LEFT JOIN bc_charge chg
					ON chg.ID = cc.ChargeID
			LEFT JOIN bc_policyperiod polper
					ON polper.HiddenTAccountContainerID = chg.TAccountContainerID
			LEFT JOIN bc_chargepattern chrgp
					ON chrgp.ID = chg.ChargePatternID
			LEFT JOIN bc_TAcctOwnerPattern c
					ON chrgp.TAccountOwnerPatternID = c.ID
			LEFT JOIN bc_billinginstruction bi
					ON bi.ID = chg.BillingInstructionID
			LEFT JOIN bctl_billinginstruction tlbi
					ON tlbi.ID = bi.subtype
			LEFT JOIN bctl_producerpayment tlpp
					ON prodpy.Subtype = tlpp.ID
			LEFT JOIN bctl_producercontext prodc
					ON prodc.ID = cix.subtype
			LEFT JOIN bc_writeoff wrto
					ON cix.WriteoffContainerID = wrto.ID
			LEFT JOIN bctl_writeoffreason worsn
					ON wrto.Reason = worsn.ID
			LEFT JOIN bc_writeoffreversal wrtor
					ON wrtor.WriteoffID = wrto.ID
			LEFT JOIN bctl_writeoffreversalreason worsnr
					ON wrtor.Reason = worsnr.ID
		),


		ItemCommission
		AS
		(
		
		Select trans.ID TxnID,trans.TransactionNumber TransactionNumber,wor.NAME Reason
From bc_writeoff WO Join bc_itemcommission IC ON WO.ItemCommissionID = IC.ID
Join bc_policycommission PC ON IC.PolicyCommissionID = PC.ID
Join bc_producercontext PCO ON PC.ProducerCodeID = PCO.ProducerCodeID
LEFT JOIN bc_transaction trans
					ON trans.ID = pco.TransactionID
Join bctl_writeoffreason WOR ON WO.Reason = WOR.ID
--WHERE trans.TransactionNumber='1000000006-0408'
group by trans.ID ,trans.TransactionNumber ,wor.NAME 

		),


     GeneralSuspense
     AS (
		SELECT b.PublicID as SuspensePublicID, a.TransactionID, Cast(ISNULL(d.Name,'') as Varchar(100)) as SuspensePaymentMethod,
		COALESCE(b.AccountNumber, b.ProducerName, b.PolicyNumber) as SuspenseAppliedTo,
		b.PaymentDate as SuspensePaymentDate, b.RefNumber as SuspenseRefNumber, b.InvoiceNumber as SuspenseInvoiceNumber,
		Cast(ISNULL(e.Name,'') as Varchar(100)) as SuspensePaymentType,
		b.Description as SuspenseDescription,
		f.Name as PaymentSource
		from bc_susppymtcontext a
		left join bc_suspensepayment b on a.SuspensePaymentID = b.ID
		left join bc_paymentinstrument c on b.PaymentInstrumentID = c.ID
		left join bctl_paymentmethod d on c.PaymentMethod = d.ID
		left join bctl_epaypaymenttype_ext e on b.EpayPaymentType_Ext = e.ID
		left join bctl_paymentsource_ext f on b.PaymentSource_Ext = f.ID
		),
     ProdAcctSuspense
     AS (
		select a.PublicID as SuspensePublicID, a.TransactionID , CheckNumber_Ext as SuspenseRefNumber, 
		PolicyNumber as SuspenseInvoiceNumber, PaymentComments as SuspenseComments,
		ExecutedDate as SuspensePaymentDate
		from bc_nonreceivableitemctx a
		left join bc_basenonrecdistitem b on a.BaseNonReceivableDistItemID = b.ID
		)
SELECT 
		trx.TransactionDate, trx.ID as TrxID,
		li.PublicID,
		trx.Amount                                                                                                            AS TransactionAmount,
		Cast(lds.NAME AS VARCHAR(50))																			              AS LedgersideName,
		btrx.TYPECODE                                                                                                         AS TransactionTypeCode,
		Cast(btrx.NAME AS VARCHAR(100))                                                                                       AS TransactionTYPE,
		Cast(trx.TransactionNumber AS VARCHAR(100))                                                                           AS TransactionNumber,
		Rtrim(tacptr.TAccountName + ' '
				+ Isnull (suffix.NAME, ''))                                                                                   AS TAccountName,
		Cast(btac.NAME AS VARCHAR(100))                                                                                       AS TAccountSubType,
		Cast(taptype.NAME AS VARCHAR(100))                                                                                    AS TAccountPatternType,
		Cast(trfrsn.NAME AS VARCHAR(100))                                                                                     AS TransactionTransferReason,
		Cast(rvrsn.NAME AS VARCHAR(100))                                                                                      AS TransactionReversalReason,
		CASE
			WHEN tacpow.TAccountOwner = 'Account' THEN 1
			ELSE 0
		END                                                                                                                   AS LedgerIndicator,
		CASE
			WHEN txnReversal.ID IS NULL THEN 0
			ELSE 1
		END                                                                                                                   AS TransactionReversalIndicator,
		Cast(tacpow.TAccountOwner AS VARCHAR(25))                                                                             AS TAccountOwner,
		Cast(Concat(li.BeanVersion, trx.BeanVersion, tac.BeanVersion, tacptr.BeanVersion, 
		tacpow.BeanVersion, cntct.BeanVersion) AS VARCHAR(20)) AS BeanVersion,
		CASE trx.Retired
			WHEN 0 THEN 1
			ELSE 0
		END                                                                                                                   AS IsActive,
		Cast(COALESCE(cix.ChargeCode, pcx.ChargeCode) AS VARCHAR(50))                                                         AS ChargeCode,
		Cast(COALESCE(cix.Context, pcx.ChargeContext) AS VARCHAR(50))                                                         AS ChargeContext,
		--Cast(COALESCE(cix.TransactionEffectiveDate, pcx.TransactionEffectiveDate) AS DATETIME) AS TransactionEffectiveDate,
		
		--Commented since the column was not available 
		Cast(COALESCE(cix.SourceSystemProcessDate, pcx.SourceSystemProcessDate) AS DATETIME)								  AS SourceSystemProcessDate,
		Cast(COALESCE(cix.ReconciledDate, pcx.ReconciledDate) AS DATETIME)													  AS ReconciledDate,
		COALESCE(cix.TransactionSequence, cix.TransactionSequence)															  AS TransactionSequence,
		li.ID                                                                                                                 AS GWRowNumber,
		pcx.ProducerPaymentMethod,
		pcx.ProducerPaymentType,
		pcx.ProducerContexttype,
		CAST(CASE WHEN cntct.Name is NULL THEN CONCAT(cntct.FirstName,', ',cntct.LastName) ELSE cntct.Name END as varchar(100)) CreateUser,
		CIX.ChargeGroup,
		trx.Reason AS TransactionReason,
		COALESCE(cix.WriteoffReason,pcx.WriteoffReason,IC.reason)																		  AS WriteoffReason,
		COALESCE(cix.ReversalWriteoffReason,pcx.ReversalWriteoffReason)														  AS ReversalWriteoffReason,
		CommissionSubPlanName,
		gensus.SuspenseAppliedTo,
		gensus.SuspensePaymentMethod,
		COALESCE(gensus.SuspensePublicID,pasus.SuspensePublicID) as TrxGroupPublicID,
		COALESCE(gensus.SuspensePaymentDate,pasus.SuspensePaymentDate) as SuspensePaymentDate,
		COALESCE(gensus.SuspenseRefNumber,pasus.SuspenseRefNumber) as SuspenseRefNumber,
		COALESCE(gensus.SuspenseInvoiceNumber,pasus.SuspenseInvoiceNumber) as SuspenseInvoiceNumber,
		gensus.SuspensePaymentType,
		COALESCE(gensus.SuspenseDescription,pasus.SuspenseComments) as SuspenseDescription,
		ProducerStatementDate,
		ProducerStatementNumber,
PaymentSource as SuspensePaymentSource
FROM   bc_lineitem li
       JOIN bc_transaction trx
         ON li.TransactionID = trx.ID
       JOIN bctl_transaction btrx
         ON trx.Subtype = btrx.ID
       JOIN bc_taccount tac
         ON li.TAccountID = tac.ID
       JOIN bc_TAccountPattern tacptr
         ON tac.TAccountPatternID = tacptr.ID
       JOIN bc_TAcctOwnerPattern tacpow
         ON tacptr.TAccountOwnerPatternID = tacpow.ID
       JOIN bctl_taccount btac
         ON tac.Subtype = btac.ID
       LEFT JOIN bctl_ledgerside lds
         ON li.Type = lds.ID
       LEFT JOIN bc_account acc
         ON tac.TAccountContainerID = acc.HiddenTAccountContainerID
       LEFT JOIN bc_plan billpl
         ON billpl.ID = acc.BillingPlanID
       LEFT JOIN bc_revtrans txnReversal
         ON trx.ID = txnReversal.OwnerID
       LEFT JOIN bc_transfertxcontext txnTrnsfr
         ON COALESCE(txnReversal.ForeignEntityID, trx.ID) = txnTrnsfr.TransactionID
       LEFT JOIN bc_fundstransfer fundsTrnsfr
         ON txnTrnsfr.FundsTransferID = fundsTrnsfr.ID
       LEFT JOIN bctl_transferreason trfrsn
         ON fundsTrnsfr.Reason = trfrsn.ID
       LEFT JOIN bctl_reversalreason rvrsn
         ON trx.ReversalReason = rvrsn.ID
       LEFT JOIN bctl_taccounttype taptype
         ON tacptr.TAccountType = taptype.ID
       LEFT JOIN ChargeInstanceContext cix
         ON cix.TxnID = trx.ID
       LEFT JOIN ProducerContext pcx
         ON pcx.TxnID = trx.ID
       LEFT JOIN bctl_taccountpatternsuffix suffix
         ON tacptr.Suffix = Suffix.ID
       LEFT JOIN bc_user crusr
         ON li.CreateUserID = crusr.id 
       LEFT JOIN bc_contact cntct
         ON crusr.ContactID = cntct.id
       LEFT JOIN GeneralSuspense gensus
         ON gensus.TransactionID = trx.id
       LEFT JOIN ProdAcctSuspense pasus
         ON pasus.TransactionID = trx.id
		 left join ItemCommission IC
		 on pcx.TxnID=IC.TxnID 
		
		
WHERE 
 (
 li.UpdateTime &gt;= Dateadd(D, ?, Cast(Getdate() AS DATE))
        OR trx.UpdateTime &gt;= Dateadd(D, ?, Cast(Getdate() AS DATE))
)</property>

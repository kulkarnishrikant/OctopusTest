  ON bd.id = COALESCE(dt.activedistid, dt.reverseddistid)
LEFT JOIN bc_policyperiod pp
  ON pp.ID = COALESCE(dt.PolicyPeriodID, ii.PolicyPeriodID)
LEFT JOIN bc_policy p
  ON pp.PolicyID = p.ID
LEFT JOIN bc_account acct
  ON p.AccountID = acct.ID
LEFT JOIN bc_plan paypl
  ON paypl.ID = pp.PaymentPlanID
LEFT JOIN bc_basemoneyreceived bmr
  ON bmr.BaseDistID = bd.ID
LEFT JOIN bc_policycommission pcom
  ON pcom.PrimaryPolicyPeriodID = pp.ID
LEFT JOIN bc_producercode pcod1
  ON pcod1.ID = pcom.ProducerCodeID
LEFT JOIN bc_producer prdc1
  ON prdc1.ID = pcod1.ProducerID
LEFT JOIN bctl_uwcompany uwc
  ON pp.UWCompany = uwc.ID
INNER JOIN bc_charge chrg
  ON COALESCE(ii.ChargeID, cic.ChargeID) = chrg.ID --Need to use a coalese because the relationships are different for Charge Reversals &amp; InvoiceItem.Charge is more accurate
UNION
SELECT
  'N' AS IsDBPayment --For Testing Purpose
  ,
  TransactionID,
  ISNULL(bmr.PublicID, '-1') AS lkp_PaymentReceivedID,
  CASE
    WHEN bmr.BaseDistID IS NOT NULL THEN ISNULL(bd.PublicID, '-1')
    ELSE ISNULL(bd.PublicID, '-2')
  END AS lkp_PaymentDistributionID,
  '-2',
  '-2',
  CASE
    WHEN pp.ID IS NULL THEN -2
    ELSE ISNULL(pp.TermNumber, -1)
  END AS TermNumber,
  CASE
    WHEN pp.ID IS NULL THEN '-2'
    ELSE ISNULL(pp.PublicID, '-1')
  END AS PolicyNumber,
  CAST
  (
  CASE
    WHEN pp.ID IS NULL THEN '1900-01-01'
    ELSE pp.PolicyPerEffDate
  END AS date
  )
  AS EffectiveDate,
  CAST
  (
  CASE
    WHEN pp.ID IS NULL THEN '1900-01-01'
    ELSE pp.PolicyPerExpirDate
  END AS date
  )
  AS ExpirationDate,
  ISNULL(paypl.PublicID, '-1') AS lkp_PaymentPlanID,
  ISNULL(uwc.TYPECODE, '-2') AS lkp_CompanyCode,
  ISNULL(prdc.PublicID, '-1') AS lkp_AgencyID,
  ISNULL(pcod.PublicID, '-1') AS lkp_ProducerID,
  '-2' AS lkp_DisbursementID,
  NULL AS StatementNumber
FROM bc_nonreceivableitemctx ct
JOIN bc_basenonrecdistitem dt
  ON ct.BaseNonReceivableDistItemID = dt.ID
JOIN bc_basedist bd
  ON bd.ID = COALESCE(dt.ActiveDistID, dt.ReversedDistID)
LEFT JOIN bc_policyperiod pp
  ON pp.ID = dt.MatchingPolicyID
LEFT JOIN bc_policy p
  ON pp.PolicyID = p.ID
LEFT JOIN bc_account acct
  ON COALESCE(p.AccountID, ct.AccountWithSuspenseID) = acct.ID
LEFT JOIN bc_plan paypl
  ON paypl.ID = pp.PaymentPlanID
LEFT JOIN bc_basemoneyreceived bmr
  ON bmr.BaseDistID = bd.ID
LEFT JOIN bc_policycommission pcom
  ON pcom.PolicyPeriodID = pp.ID
LEFT JOIN bc_producercode pcod
  ON pcod.ID = pcom.ProducerCodeID
LEFT JOIN bc_producer prdc
  ON prdc.ID = COALESCE(pcod.ProducerID, ct.ProducerWithSuspenseID)
LEFT JOIN bctl_uwcompany uwc
  ON pp.UWCompany = uwc.ID)
SELECT
  IsDBPayment,
  txn.PublicID AS TransactionPublicID,
  ISNULL(trxid.lkp_InvoiceID, '-2') AS lkp_InvoiceID,
  ISNULL(TRXID.lkp_InvoiceItemID, '-2') AS lkp_InvoiceItemID,
  ISNULL(TRXID.lkp_PaymentDistributionID, '-2') AS lkp_PaymentDistributionID,
  ISNULL(TRXID.lkp_PaymentReceivedID, '-2') AS lkp_PaymentReceivedID,
  ISNULL(TRXID.TermNumber, '-2') AS TermNumber,
  ISNULL(TRXID.PolicyNumber, '-2') AS PolicyNumber,
  ISNULL(TRXID.EffectiveDate, '1900-01-01') AS EffectiveDate,
  ISNULL(TRXID.ExpirationDate, '1900-01-01') AS ExpirationDate,
  ISNULL(TRXID.lkp_PaymentPlanID, '-2') AS lkp_PaymentPlanID,
  ISNULL(TRXID.lkp_CompanyCode, '-2') AS lkp_CompanyCode,
  ISNULL(TRXID.lkp_AgencyID, '-2') AS lkp_AgencyID,
  ISNULL(TRXID.lkp_ProducerID, '-2') AS lkp_ProducerID,
 CASE WHEN StatementNumber IS NOT NULL THEN  ISNULL(txn.PublicID+TRXID.lkp_DisbursementID, '-2') ELSE  ISNULL(TRXID.lkp_DisbursementID, '-2') END AS lkp_DisbursementID,
  StatementNumber
FROM TXN_CONTEXT txn
LEFT JOIN TRXID
  ON txn.ID = TRXID.TransactionID</property>

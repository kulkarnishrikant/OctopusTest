WITH TXN_CONTEXT
AS (SELECT
  txn.id,
  txn.publicid,
  transactionnumber,
  transactiondate,
  subtype,
  txntype.typecode AS txn_subtype,
  txn.amount,
  CASE UPPER(txntype.typecode)
    WHEN 'agencymoneyreceivedtxn' THEN COALESCE(txnreversal.foreignentityid, txn.id)
    WHEN 'directbillmoneyrcvdtxn' THEN COALESCE(txnreversal.foreignentityid, txn.id)
    WHEN 'abstractmoneyreceivedtxn' THEN COALESCE(txnreversal.foreignentityid, txn.id)
    ELSE txn.id
  END AS txn_context_id
FROM bc_transaction txn
INNER JOIN bctl_transaction txntype
  ON txn.subtype = txntype.id
LEFT JOIN bc_revtrans txnreversal
  ON txn.id = txnreversal.ownerid
WHERE (
1 = 1
AND txn.UpdateTime &gt;= DATEADD(D, ?, CAST(GETDATE() AS date))
)),

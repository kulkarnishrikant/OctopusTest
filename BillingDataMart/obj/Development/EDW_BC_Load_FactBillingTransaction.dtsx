WITH PolicyPeriod
AS (
SELECT
  pp.HiddenTAccountContainerID,
  ISNULL(prodCode.PublicID, '-2') AS ProducerCode,
  ISNULL(prod.PublicID, '-2') AS AgencyCode,
  acc.AccountNumber,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(pp.TermNumber, '-2') AS TermNumber,
  ISNULL(pp.PublicID, '-2') AS PolicyNumber,
  CAST(ISNULL(CAST(pp.PolicyPerEffDate AS date), '1900-01-01') AS varchar(50)) AS EffectiveDate,
  CAST(ISNULL(CAST(pp.PolicyPerExpirDate AS date), '1900-01-01') AS varchar(50)) AS ExpirationDate,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID,
  ISNULL(paypl.PublicID, '-2') AS lkp_PaymentPlanID,
  CAST(ISNULL(uwc.TYPECODE, '-2') AS int) AS lkp_CompanyCode
FROM BC_POLICYPERIOD pp
JOIN BC_POLICY pol
  ON pp.PolicyID = pol.ID
JOIN BC_ACCOUNT acc
  ON pol.AccountID = acc.ID
LEFT JOIN bc_policycommission pcom
  ON pcom.PrimaryPolicyPeriodID = pp.ID
  AND pcom.EndDate IS NULL
LEFT JOIN BC_PRODUCERCODE prodCode
  ON pcom.ProducerCodeID = prodCode.ID
LEFT JOIN BC_PRODUCER prod
  ON prodCode.ProducerID = prod.ID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID
LEFT JOIN bc_plan paypl
  ON paypl.ID = pp.PaymentPlanID
LEFT JOIN bctl_policyrole tlpr
  ON pcom.Role = tlpr.ID
  AND tlpr.TypeCode = 'primary'
LEFT JOIN bctl_uwcompany uwc
  ON pp.UWCompany = uwc.ID
  ),
ProducerCode
AS (SELECT
  prodcode.HiddenTAccountContainerID,
  ISNULL(prodCode.PublicID, '-2') AS ProducerCode,
  ISNULL(prod.PublicID, '-2') AgencyCode
FROM BC_PRODUCERCODE prodCode
LEFT JOIN BC_PRODUCER prod
  ON prodCode.ProducerID = prod.ID),
PolComm
AS (SELECT
  pcom.HiddenTAccountContainerID,
  acc.AccountNumber,
  ISNULL(prodCode.PublicID, '-2') AS ProducerCode,
  ISNULL(prod.PublicID, '-2') AgencyCode,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(pp.TermNumber, '-2') AS TermNumber,
  ISNULL(pp.PublicID, '-2') AS PolicyNumber,
  CAST(ISNULL(CAST(pp.PolicyPerEffDate AS date), '1900-01-01') AS varchar(50)) AS EffectiveDate,
  CAST(ISNULL(CAST(pp.PolicyPerExpirDate AS date), '1900-01-01') AS varchar(50)) AS ExpirationDate,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID,
  ISNULL(paypl.PublicID, '-2') AS lkp_PaymentPlanID,
  CAST(ISNULL(uwc.TYPECODE, '-2') AS int) AS lkp_CompanyCode
FROM bc_policycommission pcom  --199
LEFT JOIN BC_PRODUCERCODE prodCode
  ON pcom.ProducerCodeID = prodCode.ID
LEFT JOIN BC_PRODUCER prod
  ON prodCode.ProducerID = prod.ID
LEFT JOIN BC_POLICYPERIOD pp
  ON pcom.PolicyPeriodID = pp.ID
LEFT JOIN BC_POLICY pol
  ON pp.PolicyID = pol.ID
LEFT JOIN BC_ACCOUNT acc
  ON pol.AccountID = acc.ID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID
LEFT JOIN bc_plan paypl
  ON paypl.ID = pp.PaymentPlanID
LEFT JOIN bctl_uwcompany uwc
  ON pp.UWCompany = uwc.ID),
ChargeComm
AS (SELECT
  cc.HiddenTAccountContainerID,
  acc.AccountNumber,
  ISNULL(prodCode.PublicID, '-2') AS ProducerCode,
  ISNULL(prod.PublicID, '-2') AgencyCode,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(pp.TermNumber, '-2') AS TermNumber,
  ISNULL(pp.PublicID, '-2') AS PolicyNumber,
  CAST(ISNULL(CAST(pp.PolicyPerEffDate AS date), '1900-01-01') AS varchar(50)) AS EffectiveDate,
  CAST(ISNULL(CAST(pp.PolicyPerExpirDate AS date), '1900-01-01') AS varchar(50)) AS ExpirationDate,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID,
  ISNULL(paypl.PublicID, '-2') AS lkp_PaymentPlanID,
  CAST(ISNULL(uwc.TYPECODE, '-2') AS int) AS lkp_CompanyCode
FROM bc_chargecommission cc --722
LEFT JOIN bc_policycommission pcom
  ON cc.PolicyCommissionID = pcom.ID
LEFT JOIN BC_PRODUCERCODE prodCode
  ON pcom.ProducerCodeID = prodCode.ID
LEFT JOIN BC_PRODUCER prod
  ON prodCode.ProducerID = prod.ID
LEFT JOIN BC_POLICYPERIOD pp
  ON pcom.PolicyPeriodID = pp.ID
LEFT JOIN BC_POLICY pol
  ON pp.PolicyID = pol.ID
LEFT JOIN BC_ACCOUNT acc
  ON pol.AccountID = acc.ID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID
LEFT JOIN bc_plan paypl
  ON paypl.ID = pp.PaymentPlanID
LEFT JOIN bctl_uwcompany uwc
  ON pp.UWCompany = uwc.ID),
Collateral
AS (SELECT
  col.HiddenTAccountContainerID,
  acc.AccountNumber,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID
FROM bc_collateral col --722
JOIN bc_account acc
  ON col.AccountID = acc.ID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID),
CollateralReq
AS (SELECT
  colr.HiddenTAccountContainerID,
  acc.AccountNumber,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID
FROM bc_collateralrequirement colr
JOIN bc_collateral col
  ON colr.CollateralID = col.ID
JOIN bc_account acc
  ON col.AccountID = acc.ID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID),
SuspensePayment
AS (SELECT
  spp.HiddenTAccountContainerID,
  acc.AccountNumber,
  ISNULL(acc.PublicID, '-2') AS lkp_BillingAccountID,
  ISNULL(billpl.PublicID, '-2') AS lkp_BillingPlanID,
  ISNULL(pp.TermNumber, '-2') AS TermNumber,
  ISNULL(pp.PublicID, '-2') AS PolicyNumber,
  CAST(ISNULL(CAST(pp.PolicyPerEffDate AS date), '1900-01-01') AS varchar(50)) AS EffectiveDate,
  CAST(ISNULL(CAST(pp.PolicyPerExpirDate AS date), '1900-01-01') AS varchar(50)) AS ExpirationDate,
  ISNULL(pr.PublicID, '-2') AgencyCode
FROM bc_suspensepayment spp
LEFT JOIN bc_account acc
  ON spp.AccountNumber = acc.AccountNumber
LEFT JOIN bc_policyperiod pp
  ON spp.PolicyNumber = pp.PolicyNumber
LEFT JOIN bc_producer pr
  ON spp.ProducerName = pr.name
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID)

SELECT
  trx.PublicID AS TransactionPublicID,
  li.PublicID,
  CAST(CONCAT(li.BeanVersion, trx.BeanVersion) AS varchar(20)) AS BeanVersion,
  1 AS IsActive,
  tacpow.TAccountOwner,
  li.ID AS GWRowNumber,
  CASE lds.TYPECODE
    WHEN
      CASE
        WHEN taptype.NAME = 'ASSET' THEN 'DEBIT'
        WHEN taptype.NAME = 'REVENUE' THEN 'CREDIT'
        WHEN taptype.NAME = 'LIABILITY' THEN 'CREDIT'
        WHEN taptype.NAME = 'EXPENSE' THEN 'DEBIT'
        WHEN taptype.NAME = 'EQUITY' THEN 'CREDIT'
      END THEN li.Amount
    ELSE li.Amount * -1
  END AS SignedAmount,
  li.PublicID AS lkp_BillingTransactionID,
  CASE
    WHEN tacpow.TAccountOwner = 'Account' THEN ISNULL(acc.PublicID, '-2')
    WHEN tacpow.TAccountOwner = 'Collateral' THEN col.lkp_BillingAccountID
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.lkp_BillingAccountID
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.lkp_BillingAccountID
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.lkp_BillingAccountID
    WHEN tacpow.TAccountOwner = 'CollateralRequirement' THEN colr.lkp_BillingAccountID
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.lkp_BillingAccountID
    ELSE '-2'
  END AS lkp_BillingAccountID,
  CASE
    WHEN tacpow.TAccountOwner = 'Account' THEN ISNULL(billpl.PublicID, '-2')
    WHEN tacpow.TAccountOwner = 'Collateral' THEN col.lkp_BillingPlanID
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.lkp_BillingPlanID
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.lkp_BillingPlanID
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.lkp_BillingPlanID
    WHEN tacpow.TAccountOwner = 'CollateralRequirement' THEN colr.lkp_BillingPlanID
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.lkp_BillingPlanID
    ELSE '-2'
  END AS lkp_BillingPlanID,
  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.lkp_CompanyCode
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.lkp_CompanyCode
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.lkp_CompanyCode
    ELSE '-2'
  END AS lkp_CompanyCode,
  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.lkp_PaymentPlanID
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.lkp_PaymentPlanID
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.lkp_PaymentPlanID
    ELSE '-2'
  END AS lkp_PaymentPlanID,

  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.ProducerCode
    WHEN tacpow.TAccountOwner = 'ProducerCode' THEN pc.ProducerCode
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.ProducerCode
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.ProducerCode
    ELSE '-2'
  END AS lkp_ProducerID,
  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.AgencyCode
    WHEN tacpow.TAccountOwner = 'ProducerCode' THEN pc.AgencyCode
    WHEN tacpow.TAccountOwner = 'Producer' THEN ISNULL(prod.PublicID, '-1')
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.AgencyCode
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.AgencyCode
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.AgencyCode
    ELSE '-2'
  END AS lkp_AgencyID,
  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.TermNumber
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.TermNumber
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.TermNumber
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.TermNumber
    ELSE '-2'
  END AS TermNumber,
  CAST(CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.EffectiveDate
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.EffectiveDate
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.EffectiveDate
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.EffectiveDate
    ELSE '1900-01-01'
  END AS date) AS EffectiveDate,
  CAST(CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.ExpirationDate
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.ExpirationDate
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.ExpirationDate
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.ExpirationDate
    ELSE '1900-01-01'
  END AS date) AS ExpirationDate,
  CASE
    WHEN tacpow.TAccountOwner = 'PolicyPeriod' THEN pp.PolicyNumber
    WHEN tacpow.TAccountOwner = 'PolicyCommission' THEN polcomm.PolicyNumber
    WHEN tacpow.TAccountOwner = 'ChargeCommission' THEN cc.PolicyNumber
    WHEN tacpow.TAccountOwner = 'SuspensePayment' THEN suspmt.PolicyNumber
    ELSE '-2'
  END AS PolicyNumber,
  ISNULL(u.PublicID, '-2') AS lkp_CreateUserID-- added isnull function to replace -2 for NULL values
  ,cu.NAME as Currency
--into #tmp
FROM bc_lineitem li
JOIN bc_transaction trx
  ON li.TransactionID = trx.ID
JOIN bctl_transaction btrx
  ON trx.Subtype = btrx.ID
JOIN bc_taccount tac
  ON li.TAccountID = tac.ID
LEFT JOIN bctl_ledgerside lds
  ON li.Type = lds.ID
JOIN bc_TAccountPattern tacptr
  ON tac.TAccountPatternID = tacptr.ID
JOIN bc_TAcctOwnerPattern tacpow
  ON tacptr.TAccountOwnerPatternID = tacpow.ID
JOIN bctl_taccount btac
  ON tac.Subtype = btac.ID
LEFT JOIN bc_account acc
  ON tac.TAccountContainerID = acc.HiddenTAccountContainerID
LEFT JOIN bc_plan billpl
  ON billpl.ID = acc.BillingPlanID
LEFT JOIN Collateral col
  ON tac.TAccountContainerID = col.HiddenTAccountContainerID
LEFT JOIN PolicyPeriod pp
  ON tac.TAccountContainerID = pp.HiddenTAccountContainerID
LEFT JOIN PolComm polcomm
  ON tac.TAccountContainerID = polcomm.HiddenTAccountContainerID
LEFT JOIN bc_producer prod
  ON tac.TAccountContainerID = prod.HiddenTAccountContainerID
LEFT JOIN ProducerCode pc
  ON tac.TAccountContainerID = pc.HiddenTAccountContainerID
LEFT JOIN SuspensePayment suspmt
  ON tac.TAccountContainerID = suspmt.HiddenTAccountContainerID
LEFT JOIN ChargeComm cc
  ON tac.TAccountContainerID = cc.HiddenTAccountContainerID
LEFT JOIN CollateralReq colr
  ON tac.TAccountContainerID = colr.HiddenTAccountContainerID
LEFT JOIN BC_REVTRANS txnReversal
  ON trx.ID = txnReversal.OwnerID
LEFT JOIN BC_TRANSFERTXCONTEXT txnTrnsfr
  ON COALESCE(txnReversal.ForeignEntityID, trx.ID) = txnTrnsfr.TransactionID
LEFT JOIN BC_FUNDSTRANSFER fundsTrnsfr
  ON txnTrnsfr.FundsTransferID = fundsTrnsfr.ID
LEFT JOIN bctl_transferreason trfrsn
  ON fundsTrnsfr.Reason = trfrsn.ID
LEFT JOIN bctl_reversalreason rvrsn
  ON trx.ReversalReason = rvrsn.ID
LEFT JOIN bctl_taccounttype taptype
  ON tacptr.TAccountType = taptype.ID
LEFT JOIN bc_user u
  ON u.ID = trx.CreateUserID
LEFT JOIN bctl_currency cu
on li.Currency=cu.ID
WHERE (1 = 1
AND (li.UpdateTime &gt;= DATEADD(D, ?, CAST(GETDATE() AS date))
OR trx.UpdateTime &gt;= DATEADD(D, ?, CAST(GETDATE() AS date)))
)</property>

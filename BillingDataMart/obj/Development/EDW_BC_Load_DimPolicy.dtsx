            <component
              refId="Package\DFT - Load DimPolicy\OLE_SRC - GuideWire"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE_SRC - GuideWire"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">;with cte as 
(
select polper.id, 
max(CASE WHEN bi.ModificationDate IS NOT NULL THEN bi.ModificationDate
     ELSE CASE WHEN tlbi.TYPECODE in('Issuance','Rewrite','Renewal') THEN polper.PolicyPerEffDate
               ELSE CASE WHEN chg.ChargeDate IS NOT NULL THEN chg.ChargeDate
                         ELSE chg.WrittenDate
                    END
          END
END) AS TransactionEffectiveDate
from bc_charge chg 
left join bc_billinginstruction bi on bi.ID = chg.BillingInstructionID
left join bctl_billinginstruction tlbi on tlbi.ID = bi.Subtype
left join bc_policyperiod polper on COALESCE(bi.PolicyID, NewPolicyPeriodID, AssociatedPolicyPeriodID) = polper.ID
WHERE 1=1
and tlbi.TYPECODE = 'Cancellation'
group by polper.ID
)
SELECT
      [PolicyNumber]
      ,[PolicyNumberDenorm]
      ,CASE WHEN [TermConfirmed] = 1 THEN 'Y' ELSE 'N' END as [IsTermConfirmed]
      ,[PolicyNumberLongDenorm]
      ,[PolicyNumberLong]
      ,CASE WHEN pp.[HeldForInvoiceSending] = 1 THEN 'Y' ELSE 'N' END as [IsHeldForInvoiceSending]
      ,CASE WHEN pp.[UnderAudit] = 1 THEN 'Y' ELSE 'N' END as [IsUnderAudit]
      ,CASE WHEN pp.[PaymentDistributionEnabled] = 1 THEN 'Y' ELSE 'N' END as [IsPaymentDistributionEnabled]
      ,CASE WHEN pp.[PremiumFinance_Ext] = 1 THEN 'Y' ELSE 'N' END as [IsPremiumFinance]
      ,cast(sz.Name as varchar(64)) as SecurityZoneName
      ,[PolicyPerEffDate] as PolicyEffectiveDate
      ,pp.[ID] as GwRowNumber
	  ,bm.name as BillType
      ,CASE WHEN pp.[AssignedRisk] = 1 THEN 'Y' ELSE 'N' END as [IsAssignedRisk]
      ,[DBA]
      ,[FullPayDiscountUntil] as [FullPayDiscountUntilDate]
      ,[OfferNumber]
      ,cast(CONCAT(Policy.BeanVersion,pp.[BeanVersion],sz.beanversion) as varchar(30)) as BeanVersion
      ,cast(abt.Name as varchar(64)) as [AgencyBillType]
      ,CASE WHEN pp.[EligibleForFullPayDiscount] = 1 THEN 'Y' ELSE 'N' END as [IsEligibleForFullPayDiscount]
      ,pp.[Retired]
      ,[BoundDate]
      ,[TermNumber]
      ,cast(ct.Name as varchar(64)) as [CancellationType]
      ,[CancelReason]
      ,pp.[PublicID]
      ,[PriorPolicyNumber]
      ,[EquityBuffer]
      ,[DiscountedPaymentThreshold] as DiscountedPaymentThresholdAmount
      ,pp.[CreateTime]
      ,[Underwriter]
      ,cast(sd.NAME as varchar(64)) as [SubDivision]
      ,cast(jrd.TYPECODE as varchar(2)) as [RiskJurisdiction]
      ,CASE WHEN pp.[Collecting_Ext] = 1 THEN 'Y' ELSE 'N' END as [IsInCollection]
      ,[PolicyPerExpirDate] as PolicyExpirationDate
      ,CASE WHEN pp.[ChargeHeld] = 1 THEN 'Y' ELSE 'N' END as [IsChargeHeld]
      ,cast(cs.NAME as varchar(255)) [CancelStatus]
      ,ss.Name as SourceSystem
	  ,sl.NAME as [LedgerSystem]---CBS-16185
      ,cast(pcs.Name as varchar(255)) [ClosureStatus]
      ,[AuditStatus_Ext] as [AuditStatus]
	  ---,[LedgerSystem_Ext] as [LedgerSystem]
      ,[ArchiveDate]
      ,cast(cn.Name as varchar(255)) as [ConfirmationNotificationState]
      ,Case pp.Retired when 0 then 1 else 0 end as IsActive 
	  ,lob.NAME as Product
	  ,CASE WHEN cs.NAME = 'Canceled' THEN cte.TransactionEffectiveDate ELSE NULL END as CancellationDate
,pp.BC3PolicyKey_Ext
FROM dbo.bc_policyperiod pp
left join bctl_sourcesystem_ext ss on pp.SourceSystem_Ext=ss.id 
left join bctl_sourcesystem_ext sl on pp.LedgerSystem_Ext=sl.id
left join bctl_policyperiodbillingmethod bm on pp.BillingMethod=bm.ID
left Join bc_policy Policy on Policy.ID = pp.PolicyID 
Left Join bctl_lobcode lob ON Policy.Lobcode = lob.ID 
left join bc_securityzone sz on SecurityZoneID = sz.ID
left join bctl_agencybilltype_ext abt on AgencyBillType_Ext = abt.ID
left join bctl_cancellationtype ct on CancellationType = ct.ID
left join bctl_subdivision_ext sd on SubDivision_Ext = sd.ID
left join bctl_jurisdiction jrd  on RiskJurisdiction = jrd.ID
left join bctl_policycancelstatus cs on CancelStatus = cs.ID
left join bctl_policyclosurestatus pcs on ClosureStatus = pcs.ID
left join bctl_confirmationnotification cn on ConfirmationNotificationState = cn.ID
left join cte on pp.ID = cte.ID

WHERE (
	pp.UpdateTime &gt;= dateadd(d,?,cast(getdate() as date))
)</property>

                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	
		CAST(TRN.TransactionDate AS DATE) AS TransactionDate,
		RTRIM(TAP.TAccountName+' '+ ISNULL(TAS.NAME,'')) AS TAccountName,
		TRNL.TYPECODE AS TransactionType,
		CURL.NAME AS Currency,
		COUNT(TRNL.TYPECODE) AS BCRecordCount,
		SUM(TRN.AMOUNT) AS BCTransactionTotal
--Trouble shooting fields not to be a part of the report
		--,LIT.ID,
		--TAP.Subtype,  -- Will need joining to corresponding bctl table
		--TAC.SubType,  -- Will need joining to corresponding bctl table
		--TRNL.Name
--Trouble shooting fields not to be a part of the report

FROM [dbo].[bc_transaction] TRN
	LEFT JOIN dbo.bctl_transaction TRNL
		ON TRN.Subtype = TRNL.ID
	LEFT JOIN dbo.bctl_currency CURL
		ON TRN.Currency = CURL.ID
	LEFT JOIN dbo.bc_lineitem LIT
		ON TRN.ID = LIT.TransactionID
	LEFT JOIN dbo.bc_taccount TAC
		ON LIT.TAccountID = TAC.ID
	LEFT JOIN dbo.bc_TAccountPattern TAP
		ON TAC.TAccountPatternID = TAP.ID
	LEFT JOIN dbo.bctl_taccountpatternsuffix TAS
		ON TAS.ID = TAP.Suffix
		---where CAST(TRN.TransactionDate AS DATE)&gt;=?
GROUP BY 
		CAST(TRN.TransactionDate AS DATE),
		TRNL.TYPECODE,
		TRN.Currency,
		CURL.NAME,
		TAP.TAccountName,
		TAS.NAME
